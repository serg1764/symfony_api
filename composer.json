{
    "require": {
        "symfony/framework-bundle": "^7.3",
        "symfony/console": "^7.3",
        "symfony/routing": "^7.3",
        "symfony/http-client": "^7.3",
        "symfony/serializer": "^7.3",
        "symfony/messenger": "^7.3",
        "symfony/runtime": "^7.3",
        "symfony/yaml": "^7.3",
        "doctrine/orm": "^3.5",
        "doctrine/doctrine-bundle": "^2.15",
        "doctrine/migrations": "^3.9",
        "symfony/dotenv": "^7.3",
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "symfony/amqp-messenger": "^7.3",
        "symfony/monolog-bundle": "^3.10",
        "symfony/doctrine-messenger": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.3",
        "symfony/browser-kit": "^7.3",
        "symfony/css-selector": "^7.3",
        "symfony/maker-bundle": "^1.64"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:unit": "vendor/bin/phpunit --testsuite=\"Unit Tests\"",
        "test:integration": "vendor/bin/phpunit --testsuite=\"Integration Tests\"",
        "test:api": "vendor/bin/phpunit --testsuite=\"API Tests\"",
        "test:command": "vendor/bin/phpunit --testsuite=\"Command Tests\"",
        "test:coverage": "vendor/bin/phpunit --coverage-html coverage/"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
